<?xml version="1.0" encoding="UTF-8"?>
	<widget xmlns = "http://www.w3.org/ns/widgets"
		xmlns:gap = "http://phonegap.com/ns/1.0"
		id        = "com.phonegap.BeleonTours"
		versionCode=""
		version   = "1.0.0">
		<name>BeleonTours</name>
		<description>
Εφαρμογή Τηλεφωνικού καταλόγου τουριστικού Γραφείου BeleonTours.		</description>
		<author href="http://phonegap.com" email="pliahas@gmail.com">
pliahas Team
		</author>
		<preference name="phonegap-version" value="2.9.0" />
		<preference name="orientation" value="default" />
		<preference name="target-device" value="universal" />
		<preference name="fullscreen" value="true" />
		<preference name="webviewbounce" value="true" />
		<preference name="prerendered-icon" value="true" />
		<preference name="stay-in-webview" value="true" />
		<preference name="ios-statusbarstyle" value="black-opaque" />
		<preference name="detect-data-types" value="true" />
		<preference name="exit-on-suspend" value="true" />
		<preference name="show-splash-screen-spinner" value="true" />
		<preference name="auto-hide-splash-screen" value="true" />
		<preference name="disable-cursor" value="true" />
		<preference name="android-minSdkVersion" value="7" />
		<preference name="android-maxSdkVersion" value="0" />
		<preference name="android-installLocation" value="auto" />
		<preference name="splash-screen-duration" value="5000" />
		<preference name="load-url-timeout" value="20000" />
		<feature name="http://api.phonegap.com/1.0/file"/>
		<feature name="http://api.phonegap.com/1.0/notification"/>
		<feature name="http://api.phonegap.com/1.0/device"/>
		<icon src="res/icon/android/icon-96-xhdpi.png" width="96" height="96" />
		<gap:splash src="splash.png" width="320" height="480" />
		<gap:splash src="icon.png" width="480" height="320" />
		<gap:splash src="icon.png" width="640" height="960" />
		<gap:splash src="icon.png" width="960" height="640" />
		<gap:splash src="icon.png" width="768" height="1024 " />
		<gap:splash src="splash.png" width="1024" height="768" />
		<gap:splash src="splash.png" gap:platform="android" gap:density="ldpi" />
		<gap:splash src="icon.png" gap:platform="android" gap:density="mdpi" />
		<gap:splash src="icon.png" gap:platform="android" gap:density="hdpi" />
		<gap:splash src="icon.png" gap:platform="android" gap:density="xhdpi" />
		<gap:splash src="icon.png" gap:platform="blackberry" />
		<gap:splash src="icon.png" gap:platform="winphone" />
		<access origin="http://127.0.0.1*" />
	</widget>